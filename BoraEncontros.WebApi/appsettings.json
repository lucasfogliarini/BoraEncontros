{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BoraEncontros": "Server=tcp:bora-db.database.windows.net,1433;Initial Catalog=bora-encontros;Persist Security Info=False;User ID=bora;Password={password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "GoogleCalendarSettings": {
    "ApplicationName": "BoraEncontros",
    "ClientId": "{clientId}",
    "ClientSecret": "{clientSecret}",
    "TokenFolder": "token.json"
  }
}
